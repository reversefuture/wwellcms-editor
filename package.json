{
  "name": "work-well-cms",
  "description": "This is a boilerplate build with Vite, React 18, TypeScript, Vitest, Testing Library, TailwindCSS 3, Eslint and Prettier.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "start:prod": "vite --mode prod",
    "build": "tsc && vite build --mode prod",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix",
    "lint-staged": "lint-staged",
    "format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm lint:fix",
      "pnpm format"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@number-flow/react": "^0.5.9",
    "@reduxjs/toolkit": "^2.7.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "@toast-ui/editor": "^3.2.2",
    "antd": "^5.24.9",
    "axios": "^1.9.0",
    "blueimp-md5": "^2.19.0",
    "bytes": "^3.1.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "query-string": "^9.1.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.3",
    "recharts": "^2.15.3",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.87.0",
    "use-keep-state": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/blueimp-md5": "^2.18.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.19",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.3",
    "prettier": "3.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.1"
  }
}
